<table class="table">
  <tr class="table__row">
    <th class="table__header">Song</th>
    <th class="table__header">Artist</th> 
    <th class="table__header">Album</th>
    <th class="table__header">Genre</th>
    <th class="table__header">Minutes</th> 
    <th class="table__header">Seconds</th>
    <th class="table__header">Song Rating</th>
    <th class="table__header">Album Rating</th> 
  </tr>
  <% x.each do |l|
      l.each do |title,album,artist,genre,minutes,seconds,songrating,albumrating| %>
  <tr class="table__row">
  	<td class="table__data"><%= title %></td>
  	<td class="table__data"><%= album %></td>
  	<td class="table__data"><%= artist %></td>
  	<td class="table__data"><%= genre %></td>
  	<td class="table__data"><%= minutes %></td>
  	<td class="table__data"><%= seconds %></td>
  	<td class="table__data rating"><%= songrating %></td>
  	<td class="table__data rating"><%= albumrating %></td>
   </tr>
   <% end %>
   <% end %>
</table>

